{
	"name": "svelte-blog",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"generateData": "ts-node .scripts/pre/index.ts",
		"build": "ts-node .scripts/pre/index.ts && vite build && ts-node .scripts/post/index.ts",
		"report": "ts-node .scripts/pre/index.ts && vite build && ts-node .scripts/post/index.ts",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"source": "ts-node src/lib/preSource/index.ts"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.1.1",
		"@giscus/svelte": "^2.0.6",
		"@playwright/test": "^1.23.3",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.4",
		"@types/eslint": "^8.4.5",
		"@types/node": "^18.0.3",
		"@types/prettier": "^2.6.3",
		"@types/rollup-plugin-visualizer": "^4.2.1",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"autoprefixer": "^10.4.7",
		"clsx": "^1.2.1",
		"dotenv": "^16.0.1",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"feed": "^4.2.2",
		"mdsvex": "^0.10.6",
		"node-fetch": "^3.2.8",
		"path-to-regexp": "^6.2.1",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prettier-plugin-tailwindcss": "^0.1.12",
		"rehype-add-classes": "^1.0.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-external-links": "^1.0.1",
		"rehype-slug": "^5.0.1",
		"rollup-plugin-visualizer": "^5.6.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"svelte-sitemap": "^2.5.1",
		"tailwindcss": "^3.1.6",
		"ts-node": "^10.8.2",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^2.9.13"
	},
	"type": "module"
}
